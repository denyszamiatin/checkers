# function to define the color of square
def check_color(coordinates):

    row = int(coordinates[0])
    column = coordinates[1]

    column = ord(column.lower()) - 64

    if row % 2 == 0 and column % 2 == 0 or row % 2 and column % 2:
        return 'black'
    else:
        return 'white'


coordinates = input('Enter square coordinates, (1-8)(A-H)? ')

print (check_color(coordinates))
